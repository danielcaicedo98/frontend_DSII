{
  "name": "eslint-plugin-complexity",
  "version": "1.0.2",
  "description": "eslint plugin for complexity-related rules",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/fengzilong/eslint-plugin-complexity.git"
  },
  "type": "module",
  "source": "src/index.mjs",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "exports": {
    "require": "./dist/index.cjs",
    "import": "./dist/index.mjs"
  },
  "scripts": {
    "build": "microbundle --target node -f modern,esm,cjs",
    "dev": "microbundle watch --target node -f modern,esm,cjs",
    "test": "vitest run",
    "lint": "eslint src --ext .js,.mjs -f mo",
    "prepublishOnly": "pnpm run lint && pnpm run test && pnpm run build",
    "prepare": "simple-git-hooks"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm run lint && pnpm test"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/eslint": "^8.4.10",
    "eslint": "^8.26.0",
    "eslint-config-mo": "^0.5.3",
    "eslint-formatter-mo": "^1.2.0",
    "microbundle": "^0.15.1",
    "simple-git-hooks": "^2.8.1",
    "vitest": "0.12.6"
  },
  "dependencies": {
    "eslint-utils": "^3.0.0"
  }
}
